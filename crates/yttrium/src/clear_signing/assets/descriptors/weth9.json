{
  "$schema": "https://raw.githubusercontent.com/ethereum/ERCs/master/ERC-7730/schema.json",
  "context": {
    "$id": "Wrapped Ether",
    "contract": {
      "deployments": [
        { "chainId": 1, "address": "0xC02aaA39b223FE8D0A0E5C4F27eAD9083C756Cc2" }
      ]
    }
  },
  "metadata": {
    "token": {
      "symbol": "WETH",
      "name": "Wrapped Ether"
    }
  },
      "display": {
    "formats": {
      "deposit()": {
        "intent": "Wrap ETH into WETH",
        "interpolatedIntent": "Wrap {@value} into WETH",
        "fields": [
          {
            "path": "@value",
            "label": "Amount",
            "format": "tokenAmount",
            "params": { "tokenPath": "@.to" }
          }
        ],
        "required": ["@value"]
      }
    }
  }
}
